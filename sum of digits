import java.util.*;
class R192211507
{
  public static void main(String[]args)
  {
    int sum,n;
    Scanner sc=new Scanner(System.in);
    System.out.println("enter the number : ");
    n=sc.nextInt();
    for(sum=0;n!=0;n/=10)
    {
      sum+=n%10;
    }
    System.out.println("Sum of digits = "+sum);
  }
}
